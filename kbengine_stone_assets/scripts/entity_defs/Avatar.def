	<ClientMethods>
		<onStartWalk>
		</onStartWalk>

		<onStopWalk>
			<Arg>		VECTOR3			</Arg>
		</onStopWalk>

		<onJump>
		</onJump>

		<onNewTurn>
				<Arg>		ENTITY_ID		</Arg>
