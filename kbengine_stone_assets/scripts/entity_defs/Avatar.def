		<onThrowItem>
				<Arg>		ENTITY_ID		</Arg>
				<Arg>		VECTOR3			</Arg>
		</onThrowItem>

		<onResetItem>
			<Arg>		ENTITY_ID	</Arg>
		</onResetItem>

		<setHp>
			<Arg>		INT32		</Arg>
		</setHp>

		<onRecvDamage>
			<Arg>		ENTITY_ID	</Arg>
			<Arg>		INT32		</Arg>
			<Arg>		INT32		</Arg>
		</onRecvDamage>

		<onDie>
			<Arg>		ENTITY_ID	</Arg>
