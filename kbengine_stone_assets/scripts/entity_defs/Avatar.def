<root>
	<Interfaces>
		<Interface>	EntityCommon		</Interface>
	</Interfaces>

	<Properties>
		<roomKey>
			<Type>		SPACE_KEY			</Type>
			<Flags>		BASE				</Flags>
			<Persistent>		true				</Persistent>
		</roomKey>

		<name>
			<Type>		UNICODE			</Type>
			<Flags>		ALL_CLIENTS			</Flags>
			<Persistent>		true				</Persistent>
			<DetailLevel>		MEDIUM			</DetailLevel>
		</name>

		<dbid>
			<Type>		DBID				</Type>
			<Flags>		CELL_PUBLIC			</Flags>
			<Default>		0				</Default>
		</dbid>

		<HP>
			<Type>		INT32				</Type>
			<Flags>		ALL_CLIENTS			</Flags>
			<Default>		300				</Default>
		</HP>
	</Properties>

	<ClientMethods>
		<onJump>
		</onJump>

		<newTurn>
				<Arg>		ENTITY_ID		</Arg>
		</newTurn>

		<onPickUpItem>
				<Arg>		ENTITY_ID		</Arg>
		</onPickUpItem>

		<onThrowItem>
				<Arg>		ENTITY_ID		</Arg>
				<Arg>		VECTOR3			</Arg>
		</onThrowItem>
	</ClientMethods>

	<BaseMethods>
		<createCell>
			<Arg>	ENTITYCALL </Arg>
		</createCell>
	</BaseMethods>

	<CellMethods>
		<jump>
			<Exposed/>
		</jump>

		<pickUpItem>
			<Exposed/>
			<Arg>		ENTITY_ID		</Arg>
		</pickUpItem>

		<throwItem>
			<Exposed/>
			<Arg>		ENTITY_ID	</Arg>
			<Arg>		VECTOR3		</Arg>
		</throwItem>
	</CellMethods>

</root>
