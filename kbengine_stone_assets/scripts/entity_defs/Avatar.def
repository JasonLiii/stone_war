		<accountName>
			<Type>		UNICODE			</Type>
			<Flags>		ALL_CLIENTS			</Flags>
		</accountName>

		<dbid>
			<Type>		DBID				</Type>
			<Flags>		CELL_PUBLIC			</Flags>
			<Default>		0				</Default>
		</dbid>

		<HP>
			<Type>		INT32				</Type>
			<Flags>		ALL_CLIENTS			</Flags>
			<Default>		300				</Default>
		</HP>
