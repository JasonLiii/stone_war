			<Exposed/>
			<Arg>		ENTITY_ID		</Arg>
			<Arg>		VECTOR3		</Arg>
		</pickUpItem>

		<throwItem>
			<Exposed/>
			<Arg>		ENTITY_ID	</Arg>
			<Arg>		VECTOR3		</Arg>
		</throwItem>

		<newTurn>
			<Exposed/>
		</newTurn>

		<resetItem>
			<Exposed/>
			<Arg>		ENTITY_ID	</Arg>
		</resetItem>

		<recvDamage>
			<Exposed/>
			<Arg>		ENTITY_ID	</Arg>
		</recvDamage>
